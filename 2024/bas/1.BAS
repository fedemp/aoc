10 OPEN  "I",#1, "INPUT.TXT"
20 DIM L(1000): DIM R(1000)
30 FOR I=0 TO 999
40 LINE INPUT #1, LN$
50 LF=VAL(LEFT$(LN$,5)): RG=VAL(RIGHT$(LN$, 5))
60 L(I)=LF: R(I)=RG
70 NEXT I
80 REM Finished creating two lists
90 REM Clome each list to sort in subroutine
100 DIM TOSORT(1000)
110 FOR I=0 TO 999 : TOSORT(I) = L(I) : NEXT: GOSUB 180
120 FOR I=0 TO 999 : L(I) = TOSORT(I) : NEXT
130 FOR I=O TO 999 : TOSORT(I) = R(I) : NEXT: GOSUB 180
140 FOR I=0 TO 999 : R(I) = TOSORT(I) : NEXT
150 ERASE TOSORT
160 END
170 REM Bubble sort subroutine
180 N = 1000
190 WHILE N > 0
200 NN = 0
210 FOR I=1 TO N-1: IF TOSORT(I - 1) > TOSORT(I) THEN 220 ELSE 230
220 SWAP TOSORT(I-1), TOSORT(I) : N = NN
230 NEXT: WEND
240 RETURN
