10 OPEN  "I",#1, "INPUT.TXT"
20 DIM L(1000): DIM R(1000)
30 FOR I=0 TO 999
40 LINE INPUT #1, LN$
50 LF=VAL(LEFT$(LN$,5)): RG=VAL(RIGHT$(LN$, 5))
60 L(I)=LF: R(I)=RG
70 NEXT I
80 REM Finished creating two lists
90 REM Clone each list to sort in subroutine
100 DIM TOSORT(1000)
110 FOR I=0 TO 999 : TOSORT(I) = L(I) : NEXT: GOSUB 1010
120 FOR I=0 TO 999 : L(I) = TOSORT(I) : NEXT
130 FOR I=O TO 999 : TOSORT(I) = R(I) : NEXT: GOSUB 1010
140 FOR I=0 TO 999 : R(I) = TOSORT(I) : NEXT
150 ERASE TOSORT
160 REM Start comparing lists
170 TOTAL = 0
175 STOP
180 FOR I = 0 TO 999: TOTAL = TOTAL + ABS(R(I) - L(I)): NEXT
190 PRINT TOTAL
200 END
1000 REM INSERTION SORT
1010 I = 1
1020 WHILE I < 1000
1030 J = I
1040 WHILE J > 0
1050 IF TOSORT(J-1) > TOSORT(J) THEN SWAP TOSORT(J-1), TOSORT(J)
1060 J = J - 1
1070 WEND
1075 PRINT I
1080 I = I + 1
1090 WEND
1100 RETURN
