10 CLS: OPEN "i", #1, "input.txt"
20 LNS=1176: DIM L%(100,100) : TOTAL = 0
30 FOR I=0 TO LNS-1
40 LINE INPUT #1, N$
50 L%(VAL(LEFT$(N$,2)) , VAL(RIGHT$(N$,2))) = 1
60 NEXT I
70 LINE INPUT #1, N$ ' White space
80 UP = 23: DIM U%(UP)
90 IF EOF(1) THEN PRINT TOTAL : END
100 LINE INPUT #1, N$
110 ERASE U%: DIM U%(UP)
120 REM POPULATE ARRAY WITH CURRENT LINE
130 FOR I=0 TO CINT(LEN(N$)/3)
140 U%(I) = VAL(MID$(N$, (I*3)+1, 3))
150 NEXT I
160 REM START TRAVERSING ARRAY
165 INVA=0 : AGA = 0
170 FOR I=0 TO CINT(LEN(N$)/3)-2
180 C=U%(I) : N=U%(I+1)
190 IF L%(C,N) = 0 THEN INVA=1 : SWAP U%(I), U%(I+1): GOTO 170
200 NEXT I
210 IF INVA THEN TOTAL = TOTAL + U%(CINT(LEN(N$)/2/3))
215 FOR F=0 TO 5: PRINT U%(F): NEXT F
220 GOTO 90
